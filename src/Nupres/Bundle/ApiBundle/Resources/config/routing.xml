<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://symfony.com/schema/routing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">
  <route id="nupres_api_homepage" path="/">
    <default key="_controller">NupresApiBundle:Default:index</default>
  </route>
  <route id="add" path="/add">
    <default key="_controller">NupresApiBundle:User:add</default>
  </route>
  <route id="nupres_api_security_login" path="/security/login">
    <default key="_controller">NupresApiBundle:Security:login</default>
  </route>
  <route id="nupres_api_security_logout" path="/security/logout">
    <default key="_controller">NupresApiBundle:Security:logout</default>
  </route>
  <route id="nupres_api_security_isloggedin" path="/security/isloggedin">
    <default key="_controller">NupresApiBundle:Security:isloggedin</default>
  </route>
  <route id="nupres_api_security_jwt_decode" path="/security/jwt/decode">
    <default key="_controller">NupresApiBundle:Security:jwtDecode</default>
  </route>
  <route id="nupres_api_patient_add" path="/patient/add">
    <default key="_controller">NupresApiBundle:Patient:add</default>
  </route>
  <route id="nupres_api_patient_get_all" path="/patient/all">
    <default key="_controller">NupresApiBundle:Patient:getAll</default>
  </route>
  <route id="nupres_api_patient_purge" path="/patient/purge">
    <default key="_controller">NupresApiBundle:Patient:purge</default>
  </route>
  <route id="nupres_api_patient_delete" path="/patient/delete">
    <default key="_controller">NupresApiBundle:Patient:delete</default>
  </route>
  <route id="nupres_api_patient_update" path="/patient/update">
    <default key="_controller">NupresApiBundle:Patient:update</default>
  </route>
  <route id="nupres_api_patient_basic_search" path="/patient/searchbasic">
    <default key="_controller">NupresApiBundle:Patient:basicSearch</default>
  </route>
  <route id="nupres_api_config" path="/utils/config/getall">
    <default key="_controller">NupresApiBundle:Config:getAll</default>
  </route>
</routes>

<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://symfony.com/schema/routing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">
  <route id="nupres_api_homepage" path="/">
    <default key="_controller">NupresApiBundle:Default:index</default>
  </route>
  <route id="add" path="/add">
    <default key="_controller">NupresApiBundle:User:add</default>
  </route>
  <route id="nupres_api_security_login" path="/security/login" methods="POST">
    <default key="_controller">NupresApiBundle:Security:login</default>
  </route>
  <route id="nupres_api_security_logout" path="/security/logout">
    <default key="_controller">NupresApiBundle:Security:logout</default>
  </route>
  <route id="nupres_api_security_isloggedin" path="/security/isloggedin">
    <default key="_controller">NupresApiBundle:Security:isloggedin</default>
  </route>
  <route id="nupres_api_security_jwt_decode" path="/security/jwt/decode" methods="POST">
    <default key="_controller">NupresApiBundle:Security:jwtDecode</default>
  </route>
  <route id="nupres_api_patient_add" path="/patient/add">
    <default key="_controller">NupresApiBundle:Patient:add</default>
  </route>
</routes>
